<service name="FilialBankService"  scope="transportsession" >

	<description>Ein Service um die Liquidität eines Kunden zu erörtern und einen Tilgungsplan zu erstellen</description>
	<parameter name="ServiceClass" locked="false">
      de.axisbank.services.filiale.FilialBankService
   </parameter>

	<operation name="getLiquiditaet">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>

	<operation name="getRueckzahlungsPlan">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>
	
	<operation name="login">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>
	
	<operation name="logoff">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>
	
	<operation name="isLogin">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>
	<operation name="getAntragssteller">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>
	<operation name="updateAntragssteller">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>

</service>